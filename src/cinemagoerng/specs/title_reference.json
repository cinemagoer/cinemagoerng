{
  "version": "20250707",
  "url": "https://www.imdb.com/title/%(imdb_id)s/reference/",
  "doctype": "html",
  "path_type": "jmespath",
  "pre": [
    "parse_next_data"
  ],
  "rules": [
    {
      "key": "imdb_id",
      "extractor": {
        "path": "props.pageProps.aboveTheFoldData.id"
      }
    },
    {
      "key": "type_id",
      "extractor": {
        "path": "props.pageProps.aboveTheFoldData.titleType.id"
      }
    },
    {
      "key": "title",
      "extractor": {
        "path": "props.pageProps.aboveTheFoldData.originalTitleText.text"
      }
    },
    {
      "key": "primary_image",
      "extractor": {
        "path": "props.pageProps.aboveTheFoldData.primaryImage.url"
      }
    },
    {
      "key": "year",
      "extractor": {
        "path": "props.pageProps.aboveTheFoldData.releaseYear.year"
      }
    },
    {
      "key": "end_year",
      "extractor": {
        "path": "props.pageProps.aboveTheFoldData.releaseYear.endYear"
      }
    },
    {
      "key": "country_codes",
      "extractor": {
        "foreach": "props.pageProps.mainColumnData.countriesDetails.countries[*]",
        "path": "id"
      }
    },
    {
      "key": "language_codes",
      "extractor": {
        "foreach": "props.pageProps.mainColumnData.spokenLanguages.spokenLanguages[*]",
        "path": "id"
      }
    },
    {
      "key": "runtime",
      "extractor": {
        "path": "props.pageProps.aboveTheFoldData.runtime.seconds",
        "transforms": [
          "div60"
        ]
      }
    },
    {
      "key": "genres",
      "extractor": {
        "foreach": "props.pageProps.mainColumnData.genres.genres[*]",
        "path": "text"
      }
    },
    {
      "key": "taglines",
      "extractor": {
        "foreach": "props.pageProps.mainColumnData.taglines.edges[*]",
        "path": "node.text"
      }
    },
    {
      "key": "plot",
      "extractor": {
        "rules": [
          {
            "key": "key",
            "extractor": {
              "path": "props.pageProps.mainColumnData.plot.language.id"
            }
          },
          {
            "key": "value",
            "extractor": {
              "path": "props.pageProps.mainColumnData.plot.plotText.plainText"
            }
          }
        ],
        "transforms": [
          "make_dict"
        ]
      }
    },
    {
      "key": "plot_summaries",
      "extractor": {
        "rules": [
          {
            "key": "key",
            "extractor": {
              "path": "locale"
            }
          },
          {
            "key": "value",
            "extractor": {
              "foreach": "props.pageProps.mainColumnData.summaries.edges[*]",
              "path": "node.plotText.plaidHtml"
            }
          }
        ],
        "transforms": [
          "make_dict"
        ]
      }
    },
    {
      "key": "rating",
      "extractor": {
        "path": "props.pageProps.aboveTheFoldData.ratingsSummary.aggregateRating",
        "transforms": [
          "decimal"
        ]
      }
    },
    {
      "key": "vote_count",
      "extractor": {
        "path": "props.pageProps.aboveTheFoldData.ratingsSummary.voteCount"
      }
    },
    {
      "key": "top_ranking",
      "extractor": {
        "path": "props.pageProps.mainColumnData.ratingsSummary.topRanking.rank"
      }
    },
    {
      "foreach": "props.pageProps.mainColumnData.categories[*]",
      "key": {
        "path": "id",
        "transforms": [
          "credit_category"
        ]
      },
      "extractor": {
        "foreach": "section.items[*]",
        "rules": [
          {
            "key": "imdb_id",
            "extractor": {
              "path": "id"
            }
          },
          {
            "key": "name",
            "extractor": {
              "path": "rowTitle"
            }
          },
          {
            "key": "__dict__",
            "extractor": {
              "path": "attributes",
              "transforms": [
                "credit_attributes"
              ]
            }
          },
          {
            "key": "characters",
            "extractor": {
              "path": "characters"
            }
          }
        ]
      }
    },
    {
      "key": "series",
      "extractor": {
        "rules": [
          {
            "key": "imdb_id",
            "extractor": {
              "path": "props.pageProps.mainColumnData.series.series.id"
            }
          },
          {
            "key": "type_id",
            "extractor": {
              "path": "props.pageProps.mainColumnData.series.series.titleType.id"
            }
          },
          {
            "key": "title",
            "extractor": {
              "path": "props.pageProps.mainColumnData.series.series.originalTitleText.text"
            }
          },
          {
            "key": "year",
            "extractor": {
              "path": "props.pageProps.mainColumnData.series.series.releaseYear.year"
            }
          },
          {
            "key": "end_year",
            "extractor": {
              "path": "props.pageProps.mainColumnData.series.series.releaseYear.endYear"
            }
          }
        ]
      }
    },
    {
      "key": "season",
      "extractor": {
        "path": "props.pageProps.mainColumnData.series.episodeNumber.seasonNumber",
        "transforms": [
          "str"
        ]
      }
    },
    {
      "key": "episode",
      "extractor": {
        "path": "props.pageProps.mainColumnData.series.episodeNumber.episodeNumber",
        "transforms": [
          "str"
        ]
      }
    },
    {
      "key": "release_date",
      "extractor": {
        "rules": [
          {
            "key": "year",
            "extractor": {
              "path": "props.pageProps.mainColumnData.releaseDate.year"
            }
          },
          {
            "key": "month",
            "extractor": {
              "path": "props.pageProps.mainColumnData.releaseDate.month"
            }
          },
          {
            "key": "day",
            "extractor": {
              "path": "props.pageProps.mainColumnData.releaseDate.day"
            }
          }
        ],
        "transforms": [
          "date"
        ]
      }
    },
    {
      "key": "previous_episode",
      "extractor": {
        "path": "props.pageProps.mainColumnData.series.previousEpisode.id"
      }
    },
    {
      "key": "next_episode",
      "extractor": {
        "path": "props.pageProps.mainColumnData.series.nextEpisode.id"
      }
    }
  ],
  "post": [
    "unpack_dicts"
  ]
}
