{
  "version": "20240526",
  "url": "https://www.imdb.com/title/%(imdb_id)s/episodes/?season=%(season)s",
  "doctype": "html",
  "path_type": "jmespath",
  "pre": ["parse_next_data"],
  "rules": [
    {
      "key": "_page_lang",
      "extractor": {
        "path": "props.pageProps.translationContext.i18n.locale"
      }
    },
    {
      "key": "imdb_id",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.id"
      }
    },
    {
      "key": "type_id",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.titleType.id"
      }
    },
    {
      "key": "title",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.originalTitleText.text"
      }
    },
    {
      "key": "primary_image",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.primaryImage.url"
      }
    },
    {
      "key": "year",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.releaseYear.year"
      }
    },
    {
      "key": "end_year",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.releaseYear.endYear"
      }
    },
    {
      "key": "episode_count",
      "extractor": {
        "path": "props.pageProps.contentData.section.episodesWidgetData.totalEpisodes.total"
      }
    },
    {
      "key": "episodes",
      "extractor": {
        "rules": [
          {
            "key": {
              "path": "props.pageProps.contentData.section.currentSeason"
            },
            "extractor": {
              "foreach": "props.pageProps.contentData.section.episodes.items[*]",
              "rules": [
                {
                  "key": "imdb_id",
                  "extractor": {
                    "path": "id"
                  }
                },
                {
                  "key": "type_id",
                  "extractor": {
                    "path": "type"
                  }
                },
                {
                  "key": "title",
                  "extractor": {
                    "path": "titleText"
                  }
                },
                {
                  "key": "season",
                  "extractor": {
                    "path": "season"
                  }
                },
                {
                  "key": "episode",
                  "extractor": {
                    "path": "episode"
                  }
                },
                {
                  "key": "year",
                  "extractor": {
                    "path": "releaseYear"
                  }
                },
                {
                  "key": "release_date",
                  "extractor": {
                    "rules": [
                      {
                        "key": "year",
                        "extractor": {
                          "path": "releaseDate.year"
                        }
                      },
                      {
                        "key": "month",
                        "extractor": {
                          "path": "releaseDate.month"
                        }
                      },
                      {
                        "key": "day",
                        "extractor": {
                          "path": "releaseDate.day"
                        }
                      }
                    ],
                    "transforms": ["date"]
                  }
                },
                {
                  "key": "_plot",
                  "extractor": {
                    "path": "plot"
                  }
                },
                {
                  "key": "rating",
                  "extractor": {
                    "path": "aggregateRating",
                    "transforms": ["decimal"]
                  }
                },
                {
                  "key": "vote_count",
                  "extractor": {
                    "path": "voteCount"
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ],
  "post": [
    "generate_episode_map",
    "set_plot_langs"
  ]
}
