{
  "version": "20251123",
  "url": "https://www.imdb.com/title/%(imdb_id)s/episodes/?season=%(season)s",
  "doctype": "html",
  "pre": [
    "parse_next_data"
  ],
  "root": "props.pageProps",
  "rules": [
    {
      "key": "_page_lang",
      "extractor": {
        "path": "translationContext.i18n.locale"
      }
    },
    {
      "key": "series",
      "extractor": {
        "root": "contentData.entityMetadata",
        "rules": [
          {
            "key": "imdb_id",
            "extractor": {
              "path": "id"
            }
          },
          {
            "key": "type_id",
            "extractor": {
              "path": "titleType.id"
            }
          },
          {
            "key": "title",
            "extractor": {
              "path": "originalTitleText.text"
            }
          },
          {
            "key": "primary_image",
            "extractor": {
              "path": "primaryImage.url"
            }
          },
          {
            "key": "year",
            "extractor": {
              "path": "releaseYear.year"
            }
          },
          {
            "key": "end_year",
            "extractor": {
              "path": "releaseYear.endYear"
            }
          }
        ]
      }
    },
    {
      "key": "episodes",
      "extractor": {
        "root": "contentData.section.episodes",
        "foreach": "items[*]",
        "rules": [
          {
            "key": "imdb_id",
            "extractor": {
              "path": "id"
            }
          },
          {
            "key": "type_id",
            "extractor": {
              "path": "type"
            }
          },
          {
            "key": "title",
            "extractor": {
              "path": "titleText"
            }
          },
          {
            "key": "season",
            "extractor": {
              "path": "season"
            }
          },
          {
            "key": "episode",
            "extractor": {
              "path": "episode"
            }
          },
          {
            "key": "year",
            "extractor": {
              "path": "releaseYear"
            }
          },
          {
            "key": "release_date",
            "extractor": {
              "root": "releaseDate",
              "rules": [
                {
                  "key": "year",
                  "extractor": {
                    "path": "year"
                  }
                },
                {
                  "key": "month",
                  "extractor": {
                    "path": "month"
                  }
                },
                {
                  "key": "day",
                  "extractor": {
                    "path": "day"
                  }
                }
              ],
              "transforms": [
                "date"
              ]
            }
          },
          {
            "key": "plot",
            "extractor": {
              "path": "plot"
            }
          },
          {
            "key": "rating",
            "extractor": {
              "path": "aggregateRating",
              "transforms": [
                "decimal"
            ]
            }
          },
          {
            "key": "vote_count",
            "extractor": {
              "path": "voteCount"
            }
          }
        ]
      }
    }
  ],
  "post": [
    "set_episodes_series",
    "set_episodes_plot_languages",
    "build_episode_map"
  ]
}
