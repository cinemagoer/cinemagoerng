{
  "version": "20251021",
  "url": "https://www.imdb.com/title/%(imdb_id)s/taglines/",
  "doctype": "html",
  "path_type": "jmespath",
  "pre": [
    "parse_next_data"
  ],
  "rules": [
    {
      "key": "imdb_id",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.id"
      }
    },
    {
      "key": "type_id",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.titleType.id"
      }
    },
    {
      "key": "title",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.originalTitleText.text"
      }
    },
    {
      "key": "primary_image",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.primaryImage.url"
      }
    },
    {
      "key": "year",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.releaseYear.year"
      }
    },
    {
      "key": "end_year",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.releaseYear.endYear"
      }
    },
    {
      "key": "runtime",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.runtime.seconds",
        "transforms": [
          "div60"
        ]
      }
    },
    {
      "key": "genres",
      "extractor": {
        "foreach": "props.pageProps.contentData.entityMetadata.titleGenres.genres[*].genre",
        "path": "text"
      }
    },
    {
      "key": "plot",
      "extractor": {
        "rules": [
          {
            "key": "key",
            "extractor": {
              "path": "props.pageProps.translationContext.i18n.locale"
            }
          },
          {
            "key": "value",
            "extractor": {
              "path": "props.pageProps.contentData.entityMetadata.plot.plotText.plainText"
            }
          }
        ],
        "transforms": [
          "make_dict"
        ]
      }
    },
    {
      "key": "rating",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.ratingsSummary.aggregateRating",
        "transforms": [
          "decimal"
        ]
      }
    },
    {
      "key": "vote_count",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.ratingsSummary.voteCount"
      }
    },
    {
      "key": "series",
      "extractor": {
        "rules": [
          {
            "key": "imdb_id",
            "extractor": {
              "path": "props.pageProps.contentData.entityMetadata.series.series.id"
            }
          },
          {
            "key": "type_id",
            "extractor": {
              "path": "'tvSeries'"
            }
          },
          {
            "key": "title",
            "extractor": {
              "path": "props.pageProps.contentData.entityMetadata.series.series.originalTitleText.text"
            }
          },
          {
            "key": "year",
            "extractor": {
              "path": "props.pageProps.contentData.entityMetadata.series.series.releaseYear.year"
            }
          },
          {
            "key": "end_year",
            "extractor": {
              "path": "props.pageProps.contentData.entityMetadata.series.series.releaseYear.endYear"
            }
          }
        ]
      }
    },
    {
      "key": "season",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.series.episodeNumber.seasonNumber",
        "transforms": [
          "str"
        ]
      }
    },
    {
      "key": "episode",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.series.episodeNumber.episodeNumber",
        "transforms": [
          "str"
        ]
      }
    },
    {
      "key": "previous_episode_id",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.series.previousEpisode.id"
      }
    },
    {
      "key": "next_episode_id",
      "extractor": {
        "path": "props.pageProps.contentData.entityMetadata.series.nextEpisode.id"
      }
    },
    {
      "key": "taglines",
      "extractor": {
        "foreach": "props.pageProps.contentData.section.items[*]",
        "path": "htmlContent",
        "transforms": [
          "unescape"
        ]
      }
    }
  ]
}
