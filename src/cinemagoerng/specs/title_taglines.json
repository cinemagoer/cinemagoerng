{
  "version": "20251028",
  "url": "https://www.imdb.com/title/%(imdb_id)s/taglines/",
  "doctype": "html",
  "path_type": "jmespath",
  "pre": [
    "parse_next_data"
  ],
  "rules": [
    {
      "key": "taglines",
      "extractor": {
        "foreach": "props.pageProps.contentData.section.items[*]",
        "path": "htmlContent",
        "transforms": [
          "unescape"
        ]
      }
    }
  ]
}
